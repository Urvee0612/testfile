pip install requests
import requests

def get_maf(rsid):
    server = "https://rest.ensembl.org"
    ext = f"/variation/human/{rsid}?"
    headers = {"Content-Type": "application/json"}
    r = requests.get(server + ext, headers=headers)
    if not r.ok:
        return None
    decoded = r.json()
    return decoded.get("minor_allele_freq")

